{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "module": "commonjs",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "allowJs": true,
    "noImplicitOverride": true,
    "noImplicitThis": true,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution":"node",
  },
  "include": ["src/**/*.*"],
}
